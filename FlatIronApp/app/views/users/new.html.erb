<%= form_for (@user) do |f| %>
  <%= f.label :first_name%>
  <%= f.text_field :first_name %><br>
  <%= f.label :last_name%>
  <%= f.text_field :last_name %><br>
  <%= f.label :mod%>
  <%= f.text_field :mod %><br>
  <%= f.label :email%>
  <%= f.text_field :email %><br>
  <%= f.label :user_name%>
  <%= f.text_field :user_name %><br>
  <%= f.label :password%>
  <%= f.text_field :password %><br>
  <%= f.submit "Click Here to Sign Up" %><br>
<% end %>
